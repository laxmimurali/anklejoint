rowSpacing 0.6426e-3;
columnSpacing 0.6426e-3;

sliceOffset -0.2539;
rowOffset -0.051857;
columnOffset -0.170857;


smooth          yes;
weight          0.5; // 1 is no smoothing; 0 is max smoothing per iteration.
nSmoothIters    10;

